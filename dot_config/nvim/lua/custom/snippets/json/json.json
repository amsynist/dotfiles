{
  "Task": {
    "prefix": "task",
    "body": [
      "{",
      "   \"type\": \"${1:process}\",",
      "   \"label\": \"${2:name}\",",
      "   \"group\": \"${3:test}\",",
      "   \"command\": \"${4:./run_tests.sh}\",",
      "   \"args\": [",
      "     \"${5:--verbose}\"",
      "   ]",
      "}"
    ],
    "description": "Task Template"
  },
  "Overseer": {
    "prefix": "overseer",
    "body": [
      "{",
      "  \"version\": \"0.1.0\",",
      "  \"tasks\": [",
      "    {",
      "      \"type\": \"${1:process}\",",
      "      \"label\": \"${2:name}\",",
      "      \"group\": \"${3:test}\",",
      "      \"command\": \"${4:./run_tests.sh}\",",
      "      \"args\": [",
      "        \"${5:--verbose}\"",
      "      ]",
      "    }",
      "  ]",
      "}"
    ],
    "description": "Task Template"
  },
  "Snippet": {
    "prefix": "snippet",
    "body": [
      "{",
      "  \"${1:HEADER}\": {",
      "    \"prefix\": \"${2:TRIGGER}\",",
      "    \"body\": [",
      "      \"${3:EXPANTION}\"",
      "    ],",
      "    \"description\": \"${4:DESCRIPTION}\"",
      "  }",
      "}"
    ],
    "description": "Snippet Template"
  }
}
